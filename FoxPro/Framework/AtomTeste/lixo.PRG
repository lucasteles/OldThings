DEFINE POPUP mnuMain SHORTCUT RELATIVE FROM MROW(),MCOL() FONT "Arial" STYLE "N"
	DEFINE BAR 1 OF mnuMain PROMPT 'Lixo'
	ON SELECTION  BAR 1 OF mnuMain msgbox("w3132")

	DEFINE BAR 2 OF mnuMain PROMPT 'Lixo2'
		DEFINE POPUP bar2  MARGIN SHADOW COLOR SCHEME 4
		DEFINE BAR 1 OF bar2 PROMPT 'CU'
		ON SELECTION  BAR 1 OF bar2 cu(2)
	ON BAR 2 OF mnuMain ACTIVATE POPUP bar2
	
	DEFINE BAR 3 OF mnuMain PROMPT 'Lixo3'
	ON SELECTION  BAR 3 OF mnuMain msgbox("w3132")


	ACTIVATE POPUP mnuMain
